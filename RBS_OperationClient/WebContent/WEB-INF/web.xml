    <servlet-name>root</servlet-name>
    <url-pattern>/root
		</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>home</servlet-name>
    <servlet-class>com.asthvinayak.controller.HomeController
		</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>home</servlet-name>
    <url-pattern>/home
		</url-pattern>
  </servlet-mapping>
  
  
  
   <servlet>
    <servlet-name>card</servlet-name>
    <servlet-class>com.asthvinayak.controller.CardController
		</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>card</servlet-name>
    <url-pattern>/card
		</url-pattern>
  </servlet-mapping>
  
   <servlet>
    <servlet-name>loan</servlet-name>
    <servlet-class>com.asthvinayak.controller.LoanController
		</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>loan</servlet-name>
    <url-pattern>/loan
		</url-pattern>
  </servlet-mapping>
  <welcome-file-list/>
</web-app>